<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp" >
        <ImageButton 
            android:id="@+id/ibn_schedule_alert_back"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:paddingRight="30dp"
            android:paddingLeft="@dimen/standard_left_margin"
            android:background="@null"
            android:src="@drawable/icon_back"
            />
        <TextView 
            android:layout_height="fill_parent"
            android:layout_width="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center_vertical"
            android:text="sss"
            android:textSize="16sp"
            />
    </RelativeLayout>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical"
	    >
        <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />
		<ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="@dimen/standard_top_margin"
		    android:background="@color/gray_bar_launchr"
		    />
		<ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />
		<TextView 
		    android:id="@+id/tv_schedule_alert_nothing"
		    android:layout_width="match_parent"
		    android:layout_height="38dp"
		    android:gravity="center_vertical"
		    android:paddingLeft="@dimen/standard_left_margin"
		    android:text="@string/calendar_nothing"
		    android:textSize="14sp"
		    />
		<ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />	    
	    <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="@dimen/standard_top_margin"
		    android:background="@color/gray_bar_launchr"
		    />
	    <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />
	    	<TextView 
	    	android:id="@+id/tv_schedule_alert_happen"
		    android:layout_width="match_parent"
		    android:layout_height="38dp"
		    android:gravity="center_vertical"
		    android:paddingLeft="@dimen/standard_left_margin"
		    android:text="@string/calendar_happen"
		    android:textSize="14sp"
		    
		    />
	    	<ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />	
		    <TextView 
		    android:id="@+id/tv_schedule_alert_before_5"
		    android:layout_width="match_parent"
		    android:layout_height="38dp"
		    android:gravity="center_vertical"
		    android:paddingLeft="@dimen/standard_left_margin"
		    android:text="@string/calendar_before_5"
		    android:textSize="14sp"
		    />	
		    <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />
		    <TextView 
		    android:id="@+id/tv_schedule_alert_before_15"
		    android:layout_width="match_parent"
		    android:layout_height="38dp"
		    android:gravity="center_vertical"
		    android:paddingLeft="@dimen/standard_left_margin"
		    android:text="@string/calendar_before_15"
		    android:textSize="14sp"
		    />	
		    <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />
		    <TextView 
		    android:id="@+id/tv_schedule_alert_before_30"
		    android:layout_width="match_parent"
		    android:layout_height="38dp"
		    android:gravity="center_vertical"
		    android:paddingLeft="@dimen/standard_left_margin"
		    android:text="@string/calendar_before_30"
		    android:textSize="14sp"
		    />
		    <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />
		     <TextView 
		    android:id="@+id/tv_schedule_alert_before_one_hour"
		    android:layout_width="match_parent"
		    android:layout_height="38dp"
		    android:gravity="center_vertical"
		    android:paddingLeft="@dimen/standard_left_margin"
		    android:text="@string/calendar_before_one_hour"
		    android:textSize="14sp"
		    />
		    <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />	
		     <TextView 
		    android:id="@+id/tv_schedule_alert_before_two_hour"
		    android:layout_width="match_parent"
		    android:layout_height="38dp"
		    android:gravity="center_vertical"
		    android:paddingLeft="@dimen/standard_left_margin"
		    android:text="@string/calendar_before_two_hour"
		    android:textSize="14sp"
		    />
		    <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />	
		     <TextView 
		    android:id="@+id/tv_schedule_alert_before_one_day"
		    android:layout_width="match_parent"
		    android:layout_height="38dp"
		    android:gravity="center_vertical"
		    android:paddingLeft="@dimen/standard_left_margin"
		    android:text="@string/calendar_before_one_day"
		    android:textSize="14sp"
		    />
		    <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />	
		     <TextView 
		    android:id="@+id/tv_schedule_alert_before_two_day"
		    android:layout_width="match_parent"
		    android:layout_height="38dp"
		    android:gravity="center_vertical"
		    android:paddingLeft="@dimen/standard_left_margin"
		    android:text="@string/calendar_before_two_day"
		    android:textSize="14sp"
		    />
		    <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />	
		     <TextView 
		    android:id="@+id/tv_schedule_alert_before_one_week"
		    android:layout_width="match_parent"
		    android:layout_height="38dp"
		    android:gravity="center_vertical"
		    android:paddingLeft="@dimen/standard_left_margin"
		    android:text="@string/calendar_before_one_week"
		    android:textSize="14sp"
		    />
		    <ImageView 
		    android:layout_width="match_parent"
		    android:layout_height="1dp"
		    android:background="@color/gray_launchr"
		    />		
	</LinearLayout>
</LinearLayout>
