<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/grey_launchr"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15sp"
        android:background="@color/white"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="15dp"
            android:visibility="gone"
            >
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="@color/chat_below_text"
                android:text="@string/my_album"
                />
            <TextView
                android:id="@+id/tv_phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:singleLine="true"
                android:ellipsize="end"
                />
        </LinearLayout>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/grey_launchr"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="15dp"
            >
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="@color/chat_below_text"
                android:text="@string/my_collection"
                />
            <TextView
                android:id="@+id/tv_mail"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:singleLine="true"
                android:ellipsize="end"
                />
        </LinearLayout>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/grey_launchr"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="15dp"
            >
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="@color/chat_below_text"
                android:text="@string/team_address"
                />
            <TextView
                android:id="@+id/tv_team_net"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:singleLine="true"
                android:ellipsize="end"
                />
        </LinearLayout>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/grey_launchr"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="15dp"
            >
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="@color/chat_below_text"
                android:text="@string/team_name_info"
                />
            <TextView
                android:id="@+id/tv_team_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:singleLine="true"
                android:ellipsize="end"
                />
        </LinearLayout>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="@color/grey_launchr"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="15dp"
            >
            <TextView
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="@color/chat_below_text"
                android:text="@string/my_colleague_circle"
                />
            <TextView
                android:id="@+id/tv_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:singleLine="true"
                android:ellipsize="end"
                />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>