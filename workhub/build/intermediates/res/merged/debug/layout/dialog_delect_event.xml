<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="15dp"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/tv_delect_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="hehe"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/tv_delect_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_below="@+id/tv_delect_title"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="34dp"
        android:text="content"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Button
        android:id="@+id/btn_delect_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_delect_content"
        android:layout_marginTop="18dp"
        android:layout_toLeftOf="@+id/tv_delect_title"
        android:text="@string/calender_delete_no" />

    <Button
        android:id="@+id/btn_delect_canel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btn_delect_ok"
        android:layout_alignBottom="@+id/btn_delect_ok"
        android:layout_toRightOf="@+id/tv_delect_title"
        android:text="@string/calender_delete_yes" />

</RelativeLayout>
