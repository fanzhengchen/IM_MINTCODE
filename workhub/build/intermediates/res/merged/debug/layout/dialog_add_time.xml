<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
	android:background="@drawable/dialog_bg_gray_round"
    >
		<RelativeLayout
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="5dp"
			android:layout_marginTop="5dp">
			<RelativeLayout
				android:id="@+id/ll_dialog_null"
				android:layout_width="match_parent"
				android:layout_height="48dp"
				android:background="@color/light_gray"
				>

				<TextView
					android:id="@+id/iv_schedule_time_selelct_canel"
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_alignParentLeft="true"
					android:paddingRight="10dp"
					android:paddingLeft="@dimen/standard_right_margin"
					android:gravity="center"
					android:text="@string/cancel"
					android:textColor="@color/blue_launchr"
					/>
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_centerInParent="true"
					android:paddingRight="10dp"
					android:paddingLeft="@dimen/standard_right_margin"
					android:gravity="center"
					android:text="@string/calendar_timepicker_title"
					android:textColor="@color/black"
					/>
				<TextView
					android:id="@+id/iv_schedule_time_selelct_finish"
					android:layout_width="wrap_content"
					android:layout_height="fill_parent"
					android:layout_alignParentRight="true"
					android:paddingRight="10dp"
					android:paddingLeft="@dimen/standard_right_margin"
					android:gravity="center"
					android:text="@string/confirm"
					android:textColor="@color/blue_launchr"
					/>
			</RelativeLayout>

			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_below="@+id/ll_dialog_null"
				android:orientation="vertical" >
				<RelativeLayout
					android:id="@+id/rl_schedule_time_select_time1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="horizontal" >
					<ImageView
						android:id="@+id/iv_schedule_time_selelct_null"
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:background="@color/light_gray"
						/>
					<RelativeLayout
						android:id="@+id/rl_schedule_time_select_shrink"
						android:layout_below="@+id/iv_schedule_time_selelct_null"
						android:layout_width="match_parent"
						android:layout_height="40dp"
						android:background="@drawable/white_bg_selected"
						>
						<TextView
							android:id="@+id/tv_schedule_time_select_title"
							android:layout_width="wrap_content"
							android:layout_height="fill_parent"
							android:layout_alignParentLeft="true"
							android:paddingLeft="@dimen/standard_left_margin"
							android:gravity="center"
							android:text="@string/calendar_schedule_time"
							android:textSize="15sp"
							/>
						<TextView
							android:id="@+id/tv_schedule_get_time"
							android:layout_width="wrap_content"
							android:layout_height="match_parent"
							android:gravity="center"
							android:layout_toRightOf="@+id/tv_schedule_time_select_title"
							android:layout_marginLeft="10dp"
							android:text="time"
							android:textSize="14sp"
							/>
						<ImageButton
							android:id="@+id/ibn_schedule_time_select_icon"
							android:layout_width="wrap_content"
							android:layout_height="fill_parent"
							android:layout_alignParentRight="true"
							android:src="@drawable/icon_drop_down"
							android:background="@null"
							android:paddingRight="15dp"
							android:paddingLeft="@dimen/standard_right_margin"/>
					</RelativeLayout>
					<com.mintcode.launchr.timer.Timer
						android:layout_below="@+id/rl_schedule_time_select_shrink"
						android:layout_width="match_parent"
						android:layout_height="190dp"
						android:id="@+id/timer"
						>
					</com.mintcode.launchr.timer.Timer>
				</RelativeLayout>
				<LinearLayout
					android:id="@+id/content_view"
					android:orientation="vertical"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					>

				</LinearLayout>

				<ImageView
					android:id="@+id/iv_schedule_time_null"
					android:layout_width="match_parent"
					android:layout_height="@dimen/standard_top_margin"
					android:background="@color/light_gray"
					android:contentDescription="@string/app_name"
					/>
				<RelativeLayout
					android:id="@+id/rl_schedule_time_add"
					android:layout_width="match_parent"
					android:layout_height="40dp"
					android:gravity="center"
					android:clickable="true"
					android:background="@drawable/white_bg_selected"
					>
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_alignParentLeft="true"
						android:layout_marginLeft="@dimen/standard_left_margin"
						android:layout_centerVertical="true"
						android:text="@string/calendar_timepicker_addalternatedata"
						android:textSize="15sp"
						android:textColor="@color/gray_launchr"
						/>
					<ImageView
						android:id="@+id/ibn_schedule_add_timeview"
						android:layout_width="wrap_content"
						android:layout_height="fill_parent"
						android:layout_alignParentRight="true"
						android:paddingRight="11dp"
						android:paddingLeft="@dimen/standard_right_margin"
						android:background="@null"
						android:src="@drawable/icon_add_black" />
				</RelativeLayout>

				<RelativeLayout
					android:layout_width="match_parent"
					android:layout_height="58dp"
					android:background="@color/light_gray"
					android:paddingRight="10dp"
					>

					<TextView
						android:id="@+id/tv_all_day"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:gravity="center"
						android:layout_marginLeft="10dp"
						android:text="@string/is_all_day"
						android:textSize="15sp"
						android:textColor="@color/black"
						android:layout_centerVertical="true"
						android:layout_alignBaseline="@+id/cb_is_allday" />

					<CheckBox
						android:id="@+id/cb_is_allday"
						android:layout_width="40dp"
						android:layout_height="30dp"
						android:layout_marginLeft="5dp"
						android:layout_toRightOf="@id/tv_all_day"
						android:checked="true"
						android:button="@null"
						android:layout_centerVertical="true"
						android:background="@drawable/checkbox_seletor_blue"
						/>

					<TextView
						android:id="@+id/tv_schedule_dialog_copy"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/calendar_timepicker_copyAlternatedata"
						android:layout_alignParentRight="true"
						android:layout_centerVertical="true"
						android:textSize="15sp"
						android:textColor="@color/blue_lite"
						android:background="@drawable/btn_copy_border"
						android:clickable="true"
						/>
				</RelativeLayout>
			</LinearLayout>
		</RelativeLayout>

</RelativeLayout>